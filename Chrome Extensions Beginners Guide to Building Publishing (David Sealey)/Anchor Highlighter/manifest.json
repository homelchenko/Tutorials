{
    "manifest_version": 2,
    "name": "Anchor Highlighter",
    "description": "Highlights anchors on the page",
    
    "browser_action": {
        "default_title": "Highlight anchors now!"
    },
    
    "version": "1",
    
    "background": {
        "scripts": ["background.js"]
    },
    
    "permissions": [
        "activeTab"
    ]
}